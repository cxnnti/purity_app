  color: black;
  display: inline-block;
  padding: 0 15px;
  cursor: pointer;
}

 .oclass{
    color: #8c8c8c;
    text-transform: capitalize;
   }


   .navhov:hover{
 border-bottom: 5px solid #ddd;
 color: #42ade7; 
 background: white;
}




.navhovactive{
